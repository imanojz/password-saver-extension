{
    "name": "Gmail Store",
    "description": "This tool is useful for Gmail Store.",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
        "default_popup": "popup.html",
        "default_icon": "/image/Gmail_new_Logo_32.png",
        "default_title": "Password Saver" 
    },
    "icons": {
        "16": "/image/Gmail_new_Logo_16.png",
        "32": "/image/Gmail_new_Logo_32.png",
        "48": "/image/Gmail_new_Logo_48.png",
        "128": "/image/Gmail_new_Logo_128.png"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["jquery.js", "content.js"],
        "run_at": "document_idle"
      }
    ],
    "permissions": ["storage", "activeTab", "scripting"]
}